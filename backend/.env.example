# DashScope API 配置（必填）
DASHSCOPE_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# 数据库配置
DATABASE_URL=sqlite:///./data.db
SQLALCHEMY_ECHO=false

# CORS 配置
CORS_ORIGINS=http://localhost:3000

# 文件上传配置
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_TYPES=["image/png", "image/jpeg", "image/jpg"]

# 日志配置
LOG_LEVEL=INFO
